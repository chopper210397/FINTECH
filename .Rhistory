filter(month(`Closed at`)==6) %>%
mutate(dia=(day(`Closed at`)-day(`Created at`))) %>%
select(dia) %>% na.omit() %>% arrange(dia) %>% top_n(n=60) %>% summarise(promedio=mean(dia))
